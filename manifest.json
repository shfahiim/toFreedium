{
  "manifest_version": 3,
  "name": "toFreedium",
  "version": "1.0",
  "description": "Redirects to Freedium.",
  "permissions": ["tabs", "contextMenus"],
  "action": {
    "default_title": "Open With Freedium",
    "default_icon": "assets/icon.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "assets/icon.png",
    "32": "assets/icon.png",
    "48": "assets/icon.png",
    "128": "assets/icon.png"
  }
}

